select t.id as id, t.from_id as from_id, t.date as taskDate, t.time as taskTime, t.task as task, u1.user_name as currUserName, u2.user_name as userName 
              from tasks as t left join users as u1 on t.id = u1.id left join users as u2 on t.from_id = u2.id
              where t.from_id='9' order by taskDate desc, taskTime desc